void main() {
//   ****** Números ******
  int empleados = 10;
//   int es solo para enteros sin decimales.
  double pi = 3.141592;
//   double es para números con decimales.
  var numero = 1.0;

  print('$empleados - $pi - $numero');

//   ****** String ******
  String nombre = 'Tony';

  print(nombre);
  print(nombre[0]);
  print(nombre[nombre.length - 1]);
}
